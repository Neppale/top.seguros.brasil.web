
@{
    ViewData["Title"] = "UserPage";
}

<body onload="getUserData()">
    <div class="card">

        <h1>Suas Coberturas</h1>

        <div class="card-body">
            <h5 id="coverageTitle" class="card-title">Título do card</h5>
            <p id="coverageDescription" class="card-text">Este é um card mais longo com suporte a texto embaixo, que funciona como uma introdução a um conteúdo adicional. Este conteúdo é um pouco maior.</p>
            <p id="coverageStatus" class="card-text"><small class="text-muted">Atualizados 3 minutos atrás</small></p>
            <p id="coveragePrice" class="card-text"><small class="text-muted">Atualizados 3 minutos atrás</small></p>
        </div>

    </div>


    <script src="~/js/UserPage.js" asp-append-version="true"></script>
</body>