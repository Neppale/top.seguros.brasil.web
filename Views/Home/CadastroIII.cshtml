<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TopSegurosBrasilWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TopSegurosBrasilWeb.styles.css" asp-append-version="true" />
</head>
<body>

    <div class="container">

        <div class="input-group input-group-lg size">
            <input type="text" class="form-control inp" id="Email" placeholder="Email" aria-label="Email" aria-describedby="basic-addon1">
        </div>

        <div class="input-group input-group-lg size">
            <input type="password" class="form-control inp" id="Senha" placeholder="Senha" aria-label="Password" aria-describedby="basic-addon1">
        </div>

        <div class="input-group input-group-lg size">
            <input type="password" class="form-control inp" id="confirmeSenha" placeholder="Confirme a senha" aria-label="Password" aria-describedby="basic-addon1">
        </div>

        <div class="size">
            <a class="cadastro-link"><button type="button" onclick="pagina3()" class="btn input-group input-group-lg">ENTRAR</button></a>
        </div>

    </div>
    <script src="~/js/Cadastro3.js" asp-append-version="true"></script>

</body>
