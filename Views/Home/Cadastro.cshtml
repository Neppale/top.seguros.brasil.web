<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TopSegurosBrasilWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TopSegurosBrasilWeb.styles.css" asp-append-version="true" />


</head>
<body>

    <div class="container">
        <div class="input-group input-group-lg size">
            <input type="text" class="form-control inp" id="nomeCompleto" placeholder="Nome completo" aria-label="NomeCompleto" aria-describedby="basic-addon1">
        </div>

        <div class="input-group input-group-lg size">
            <input type="text" class="form-control inp" id="dataNascimento" placeholder="Data de nascimento" maxlength="10" autocapitalize="off" aria-label="DataNascimento" aria-describedby="basic-addon1" onload="formatarData()">
            
        </div>

        <div class="size">
            <a class="cadastro-link"> <button type="button" onclick="cadastraCliente()" class="btn input-group input-group-lg" href="/Home/CadastroII">Continuar</button> </a>
        </div>

    </div>

    <script src="~/js/Cadastro.js" asp-append-version="true"></script>
</body>
